    <property name="message_type">error</property>
    <property name="buttons">close</property>
    <property name="text" translatable="yes">There was an error parsing the requested file.</property>
    <property name="secondary_text" translatable="yes">Please make sure you present a file in the Kindle's "My Clippings.txt" format.</property>
    <child internal-child="vbox">
      <object class="GtkVBox" id="dialog-vbox3">
        <property name="visible">True</property>
        <property name="orientation">vertical</property>
